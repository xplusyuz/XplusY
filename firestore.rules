service cloud.firestore { match /databases/{database}/documents { function isSignedIn(){return request.auth != null;} function isAdminEmail(){ return isSignedIn() && request.auth.token.email in ["sohibjonmath@gmail.com"]; } match /catalog/{docId} { allow read: if true; allow create, update, delete: if isAdminEmail(); } }}